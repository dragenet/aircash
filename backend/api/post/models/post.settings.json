{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Content": {
      "type": "richtext"
    },
    "HeroImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Tags": {
      "collection": "tags",
      "via": "posts",
      "dominant": true
    },
    "Author": {
      "via": "Posts",
      "model": "authors"
    }
  }
}
